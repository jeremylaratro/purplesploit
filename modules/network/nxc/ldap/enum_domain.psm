#!/bin/bash
# NXC LDAP Domain Enumeration Module

MODULE_NAME="network/nxc/ldap/enum_domain"
MODULE_CATEGORY="network"
MODULE_DESCRIPTION="Enumerate Active Directory domain via LDAP"
MODULE_AUTHOR="PurpleSploit Team"
MODULE_TOOL="nxc"

REQUIRED_VARS="RHOST,USERNAME"
OPTIONAL_VARS="PASSWORD:,DOMAIN:,HASH:"

COMMAND_TEMPLATE="nxc ldap \${RHOST} -u \${USERNAME} -p '\${PASSWORD}' \${DOMAIN:+-d \$DOMAIN} \${HASH:+-H \$HASH} --users --groups"
