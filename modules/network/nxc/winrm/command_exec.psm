#!/bin/bash
# NXC WinRM Command Execution Module

MODULE_NAME="network/nxc/winrm/command_exec"
MODULE_CATEGORY="network"
MODULE_DESCRIPTION="Execute command on target via WinRM"
MODULE_AUTHOR="PurpleSploit Team"
MODULE_TOOL="nxc"

REQUIRED_VARS="RHOST,USERNAME,COMMAND"
OPTIONAL_VARS="PASSWORD:,DOMAIN:,HASH:"

COMMAND_TEMPLATE="nxc winrm \${RHOST} -u \${USERNAME} -p '\${PASSWORD}' \${DOMAIN:+-d \$DOMAIN} \${HASH:+-H \$HASH} -x '\${COMMAND}'"
