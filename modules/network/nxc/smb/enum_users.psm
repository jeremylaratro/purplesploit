#!/bin/bash
# NXC SMB User Enumeration Module

MODULE_NAME="network/nxc/smb/enum_users"
MODULE_CATEGORY="network"
MODULE_DESCRIPTION="Enumerate domain users via SMB"
MODULE_AUTHOR="PurpleSploit Team"
MODULE_TOOL="nxc"

REQUIRED_VARS="RHOST,USERNAME"
OPTIONAL_VARS="PASSWORD:,DOMAIN:,HASH:"

COMMAND_TEMPLATE="nxc smb \${RHOST} -u \${USERNAME} -p '\${PASSWORD}' \${DOMAIN:+-d \$DOMAIN} \${HASH:+-H \$HASH} --users"
