#!/bin/bash
# Feroxbuster Basic Scan Module

MODULE_NAME="web/feroxbuster/basic_scan"
MODULE_CATEGORY="web"
MODULE_DESCRIPTION="Basic directory and file discovery scan"
MODULE_AUTHOR="PurpleSploit Team"
MODULE_TOOL="feroxbuster"

REQUIRED_VARS="TARGET_URL"
OPTIONAL_VARS="THREADS:50,WORDLIST:/usr/share/wordlists/dirb/common.txt"

COMMAND_TEMPLATE="feroxbuster -u \${TARGET_URL} --thorough --methods GET,POST -t \${THREADS} -w \${WORDLIST}"
