#!/bin/bash
# HTTPX URL Probing Module

MODULE_NAME="web/httpx/probe_urls"
MODULE_CATEGORY="web"
MODULE_DESCRIPTION="Probe URLs and extract titles, status codes, tech stack"
MODULE_AUTHOR="PurpleSploit Team"
MODULE_TOOL="httpx"

REQUIRED_VARS="TARGET_URL"
OPTIONAL_VARS="THREADS:50"

COMMAND_TEMPLATE="echo \${TARGET_URL} | httpx -title -status-code -tech-detect -follow-redirects -threads \${THREADS}"
